		"url": "git+https://github.com/gramiojs/views.git"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"devDependencies": {
		"@biomejs/biome": "^2.1.1",
		"@gramio/test": "^0.3.1",
		"@types/bun": "^1.2.18",
		"gramio": "^0.6.1",
		"pkgroll": "^2.14.1",
		"typescript": "^5.8.3"
	},
	"peerDependencies": {
		"gramio": "^0.4.10"
	},
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./json": {
			"import": {
				"types": "./dist/json.d.ts",
				"default": "./dist/json.js"
			},
			"require": {
				"types": "./dist/json.d.cts",
				"default": "./dist/json.cjs"
			}
		},
		"./fs": {
			"import": {
				"types": "./dist/fs.d.ts",
